{
  "imports": {
    "hono": "https://deno.land/x/hono@v3.12.5/mod.ts",
    "hono/middleware": "https://deno.land/x/hono@v3.12.5/middleware.ts",
    "hono/helper": "https://deno.land/x/hono@v3.12.5/helper.ts",
    "hono/jsx/jsx-runtime": "https://deno.land/x/hono@v3.12.5/jsx/jsx-runtime.ts",
    "cheerio": "https://esm.sh/cheerio@1.0.0-rc.12"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
  "tasks": {
    "dev": "deno task dev:server & deno task dev:css",
    "dev:server": "deno run --allow-read --allow-write --allow-net --allow-env --watch main.ts",
    "dev:css": "pnpm dlx tailwindcss -i ./public/input.css -o ./public/style.css --minify --watch"
  }
}
